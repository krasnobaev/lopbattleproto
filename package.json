    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "shipit-cli": "^3.0.0",
    "shipit-deploy": "^2.5.1",
    "ssh-pool": "^4.0.0"
  },
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "babel-polyfill": "^6.26.0",
