{
  "name": "lopbattleproto",
  "version": "0.0.3",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-bfield": "parcel build src/battlefield/index.html",
    "start-bfield": "parcel src/battlefield/index.html",
    "start-wbench": "cd src/services && npm start",
    "start-wbench-dbg": "cd src/services && npm run debug",
    "deploy": "shipit staging deploy && shipit staging inject-env",
    "deploy-bfield": "shipit staging deploy-bfield && shipit staging start-bfield&",
    "deploy-wbench": "shipit staging deploy-wbench && shipit staging start-wbench&",
    "deploy-wbench-dbg": "shipit staging deploy-wbench && shipit staging start-wbench-dbg&",
    "deploy-clean": "git push && npm run deploy && npm run deploy-bfield && npm run deploy-wbench"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-react-display-name": "^7.0.0-beta.40",
